    "@vue/tsconfig": "^0.4.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "eslint": "^8.39.0",
